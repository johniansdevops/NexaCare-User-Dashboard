{
  "assessment_id": "fitness_readiness",
  "name": "Fitness Readiness Evaluation",
  "description": "Measures baseline fitness level and readiness for starting a workout program",
  "version": "1.0",
  "questions": [
    {
      "id": 7,
      "question": "What is your current fitness level?",
      "type": "multiple_choice",
      "options": ["Beginner (no regular exercise)", "Novice (some exercise)", "Intermediate (regular exercise)", "Advanced (intense training)", "Athlete level"],
      "weight": 5,
      "category": "current_fitness"
    },
    {
      "id": 8,
      "question": "How many days per week do you currently exercise?",
      "type": "multiple_choice",
      "options": ["0 days", "1-2 days", "3-4 days", "5-6 days", "7 days"],
      "weight": 4,
      "category": "exercise_frequency"
    },
    {
      "id": 9,
      "question": "What types of exercise do you currently do?",
      "type": "checkbox",
      "options": ["No current exercise", "Walking", "Running/Jogging", "Cycling", "Swimming", "Weight training", "Group fitness classes", "Sports"],
      "weight": 3,
      "category": "exercise_types"
    },
    {
      "id": 10,
      "question": "How long can you walk briskly without getting winded?",
      "type": "multiple_choice",
      "options": ["Less than 5 minutes", "5-10 minutes", "10-20 minutes", "20-30 minutes", "30+ minutes"],
      "weight": 4,
      "category": "cardio_endurance"
    },
    {
      "id": 11,
      "question": "Can you climb 2 flights of stairs without significant breathlessness?",
