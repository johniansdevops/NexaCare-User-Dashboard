      "category": "learning_comfort"
    },
    {
      "id": 27,
      "question": "Do you prefer structured workouts or flexible routines?",
      "type": "multiple_choice",
      "options": ["Highly structured", "Somewhat structured", "Flexible with guidelines", "Very flexible", "No preference"],
